{
  "name": "live-stream-dashboard",
  "version": "1.0.0",
  "description": "Electron app for live streaming dashboard",
  "main": "main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "electron .",
    "electron:dev": "concurrently -k -s first -n \"VITE,ELECTRON\" -c \"cyan,magenta\" \"npm run dev\" \"wait-on http://localhost:5175 -t 60000 && electron .\"",
    "electron:build": "vite build && electron .",
    "generate-icon": "node assets/generate-icon.js",
    "create-icon": "node assets/create-default-icon.js",
    "postinstall": "node scripts/postinstall.js",
    "setup:windows": "npm install -g windows-build-tools",
    "setup:check": "node scripts/check-build-tools.js"
  },
  "keywords": [
    "electron",
    "live-stream",
    "dashboard"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "electron": "^28.0.0",
    "vite": "^5.0.8",
    "concurrently": "^8.2.2",
    "wait-on": "^7.2.0"
  }
}

